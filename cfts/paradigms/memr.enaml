                    autogenerate_keys = 'tab'
                    plot_keys = [(k,) for k in range(C.elicitor_n + 1)]
                    tab_grouping << C.get_names()
                    apply_calibration = True
                    fmt_plot_label_cb = lambda x, s: f'Elicitor {x[0]}'

        FFTContainer:
            name = 'memr_fft_container'
            label = 'MEMR'
